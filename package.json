{
  "name": "nextcap",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "concurrently 'npm:dev:*' -r --kill-others-on-fail",
    "dev-remote": "concurrently 'npm:dev:*(!cms-server)' -r --kill-others-on-fail",
    "dev:next": "next dev",
    "dev:gen": "ts-node-dev --respawn -r tsconfig-paths/register src/cms/types/generate-script.ts",
    "dev:cms-server": "decap-server",
    "dev:cnv": "node -e \"if(!require('semver').satisfies(x=process.versions.node,r=require('./package.json').engines.node))throw'Wrong Node version '+x+', must satisfy '+r\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@plaiceholder/next": "^3.0.0",
    "decap-cms-app": "3.1.8",
    "glob": "^10.3.12",
    "iso-639-1-plus": "^3.0.4",
    "markdown-to-jsx": "^7.4.5",
    "netlify-ts": "^2.0.0",
    "next": "14.1.4",
    "next-nprogress-bar": "^2.3.11",
    "react": "^18",
    "react-dom": "^18",
    "rtl-detect": "^1.1.2",
    "server-only": "^0.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/rtl-detect": "^1.0.3",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "decap-server": "^3.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "postcss-nesting": "^12.1.1",
    "prettier": "3.2.5",
    "semver": "^7.6.0",
    "tailwindcss": "^3.3.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.3.3"
  }
}
